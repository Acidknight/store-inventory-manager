<h1>Login here!</h1>

<%= flash[:message] if flash[:message]%>

<%= form_for :user do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Log in" %>

<% end %>
<br><br>
<%= link_to "Log In With Google", '/auth/google_oauth2'%>
<br><br>
<%= link_to "Sign Up Here", signup_path %>