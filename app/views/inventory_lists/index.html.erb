<div>
  <h4>Search for an Inventory List: </h4>
  <%= form_tag(inventory_lists_path, method: 'get') do %>
    <%= text_field_tag(:q) %>
    <%= submit_tag("search") %>
  <% end %>
</div>


<div>
    <ul>
      <% @inventory_lists.each do |inventory_list| %>
        <h2>Name: <%= link_to inventory_list.name, inventory_list_path(inventory_list) %></h2>
        Date:<%= inventory_list.date %> 
        <br>
        Time:<%= inventory_list.time %> 
        <br>
        by: <%= inventory_list.user.username %></h6>
      <% end %>
    </ul> 
</div> 